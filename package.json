{
  "name": "dbots",
  "version": "5.2.0",
  "description": "Discord bot list poster and stats retriever",
  "main": "./src/index",
  "types": "./typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbots-pkg/dbots.js.git"
  },
  "scripts": {
    "build": "tsc",
    "changelog": "ts-node scripts/changelog",
    "docs": "typedoc --json .tmp/typedoc-out.json --plugin typedoc-plugin-as-member-of && ts-node scripts/docs",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "gpr": "ts-node scripts/gpr",
    "webpack": "ts-node scripts/services-wp && cross-env NODE_ENV=production webpack && rm -r .tmp",
    "prepublishOnly": "npm run eslint:fix && npm run tslint:fix && npm run webpack"
  },
  "husky": {
    "hooks": {
      "precommit": "npm run build && npm run lint && git add lib"
    }
  },
  "unpkg": "./webpack/dbots.min.js",
  "jsdelivr": "./webpack/dbots.min.js",
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "discord bots",
    "discord bot",
    "wrapper"
  ],
  "author": "Snazzah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dbots-pkg/dbots.js/issues"
  },
  "homepage": "https://dbots.js.org",
  "dependencies": {
    "axios": "^0.21.0",
    "promise.allsettled": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.5",
    "@types/promise.allsettled": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "dtslint": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jsdoc-typeof-plugin": "^1.0.0",
    "terser-webpack-plugin": "^3.0.0",
    "ts-docgen": "github:dbots-pkg/ts-docgen#main",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsd-jsdoc": "github:EndBug/tsd-jsdoc",
    "tslint": "^6.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-as-member-of": "^1.0.2",
    "typescript": "v4.0.x",
    "webpack": "^4.42.0",
    "webpack-cli": "^4.0.0"
  }
}
