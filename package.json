{
  "name": "dbots",
  "version": "7.0.0",
  "description": "Discord bot list poster and stats retriever",
  "main": "./lib/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbots-pkg/dbots.js.git"
  },
  "scripts": {
    "build": "rm -r lib/* && tsc",
    "changelog": "ts-node scripts/changelog && npm i --package-lock-only",
    "docs": "mkdirp .tmp && typedoc --json .tmp/typedoc-out.json --plugin typedoc-plugin-as-member-of src && ts-node scripts/docs",
    "gpr": "ts-node scripts/gpr",
    "lint": "npx eslint --ext .ts ./src",
    "lint:fix": "npx eslint --ext .ts ./src --fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "npm run lint:fix && npm run webpack && pinst --disable",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "webpack": "ts-node scripts/services-wp && cross-env NODE_ENV=production webpack && rm -r .tmp"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "unpkg": "./webpack/dbots.min.js",
  "jsdelivr": "./webpack/dbots.min.js",
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "discord bots",
    "discord bot",
    "wrapper"
  ],
  "author": "Snazzah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dbots-pkg/dbots.js/issues"
  },
  "homepage": "https://dbots.js.org",
  "dependencies": {
    "axios": "^0.21.0",
    "promise.allsettled": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.5",
    "@types/promise.allsettled": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jsdoc-typeof-plugin": "^1.0.0",
    "lint-staged": "^10.5.1",
    "mkdirp": "^1.0.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.0",
    "terser-webpack-plugin": "^3.0.0",
    "ts-docgen": "github:dbots-pkg/ts-docgen#main",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typedoc": "0.19.2",
    "typedoc-plugin-as-member-of": "^1.0.2",
    "typescript": "v4.0.x",
    "webpack": "^4.42.0",
    "webpack-cli": "^4.0.0"
  }
}
